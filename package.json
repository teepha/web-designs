{
  "name": "web-designs",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass natours/sass/main.scss natours/css/style.css -w",
    "dev:server": "live-server",
    "start": "npm-run-all --parallel dev:server watch:sass",
    "compile:sass": "node-sass natours/sass/main.scss natours/css/style.comp.css",
    "concat:css": "concat -o natours/css/style.concat.css natours/css/icon-font.css natours/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' natours/css/style.concat.css -o natours/css/style.prefix.css",
    "compress:css": "node-sass natours/css/style.prefix.css natours/css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teepha/web-designs.git"
  },
  "author": "teepha",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/teepha/web-designs/issues"
  },
  "homepage": "https://github.com/teepha/web-designs#readme",
  "devDependencies": {
    "autoprefixer": "^9.7.3",
    "concat": "^1.0.3",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0"
  }
}
